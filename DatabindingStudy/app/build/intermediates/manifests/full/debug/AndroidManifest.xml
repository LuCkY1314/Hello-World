            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.chansiqing.databindingstudy.activity.ComplexFloorActivity" />
        <activity android:name="com.example.chansiqing.databindingstudy.activity.MixListActivity" />
        <activity android:name="com.example.chansiqing.databindingstudy.activity.MixListNewActivity" />
        <activity android:name="com.example.chansiqing.databindingstudy.activity.MyPageViewActivity" />
